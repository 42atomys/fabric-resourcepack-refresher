{
  "authors": ["@42atomys"],
  "contributors": ["@42atomys"],
  "contact": {
    "homepage": "https://github.com/42atomys/fabric-resourcepack-refresher",
    "sources": "https://github.com/42atomys/fabric-resourcepack-refresher",
