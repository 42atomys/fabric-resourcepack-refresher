  },
  "depends": {
    "fabric": "*",
    "fabricloader": ">=0.11.3"
  },
  "description": "Fabric Resource Pack Refresher is a mod for Minecraft that allows server administrators to refresh the resource pack on a client's game without the need for the client to relog. This can be particularly useful for servers that frequently update their resource packs.",
  "entrypoints": {
    "main": [
      "codes.atomys.resourcepackrefresher.ResourcePackRefresher"
    ]
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.github_releases": "https://github.com/42atomys/fabric-resourcepack-refresher/releases",
        "modmenu.modrinth": "https://modrinth.com/mod/resource-pack-refresher",
        "modmenu.github_sponsors": "https://github.com/sponsors/42atomys"
      },
      "update_checker": true
    }
  },
  "environment": "*",
  "icon": "assets/resourcepackrefresher/icon.png",
  "id": "resourcepackrefresher",
  "license": "MIT",
  "mixins": [
    {
      "config": "rpr.mixins.json",
      "environment": "*"
    }
  ],
  "name": "ResourcePacks Refresher",
  "schemaVersion": 1,
  "version": "1.2.0-1.21"
}
